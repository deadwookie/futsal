{
  "name": "Futsal",
  "version": "0.1.0",
  "description": "Futsal players and tournaments management",
  "keywords": [
    "Futsal",
    "Football",
    "Tournament"
  ],
  "author": {
    "name": "Pavel Zet",
    "email": "zetorama@gmail.com",
    "url": ""
  },
  "homepage": "http://futsal-bzik.rhcloud.com/",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.0.0"
  },
  "dependencies": {
    "request": "~2.39.0",
    "nconf": "~0.6.9",
    "express": "~4.7.2",
    "morgan": "~1.2.2",
    "compression": "~1.0.9",
    "express-session": "~1.7.2",
    "connect-firebase": "~0.0.6",
    "connect-redis": "^2.0.0",
    "cookie-parser": "^1.3.2",
    "serve-static": "~1.4.3",
    "serve-favicon": "~2.0.1",
    "firebase": "~1.0.18",
    "firebase-client": "tinj/firebase-client",
    "firebase-simple-login": "tinj/firebase-simple-login",
    "emberfire": "~1.1.3",
    "MD5": "~1.2.1",
    "jquery": "^2.1.1",
    "handlebars": "^1"
  },
  "devDependencies": {
    "bower": "~1.3.8",
    "gulp": "^3.8.7",
    "browserify": "^4",
    "browserify-shim": "^3.6.0",
    "watchify": "~0.10.2",
    "vinyl-source-stream": "^0.1.1",
    "hbsfy": "git+https://github.com/knownasilya/node-hbsfy.git#281ad8f1cfdbe89da84b4ea52c31087a9c643dbe",
    "gulp-nodemon": "^1.0.4",
    "gulp-util": "^3.0.0",
    "pretty-hrtime": "^0.2.1",
    "ember-template-compiler": "^1.6.1"
  },
  "bundleDependencies": [],
  "scripts": {
    "start": "node server",
    "boweri": "export HOME=$HOME/app-root/runtime; ./node_modules/bower/bin/bower install --silent"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
  },
  "browser": {
    "ember": "./bower/ember/ember.js",
    "ember-data": "./bower/ember-data/ember-data.js"
  },
  "browserify-shim": {
    "ember": {
      "exports": "Ember",
      "depends": [
        "jquery:jQuery",
        "handlebars:Handlebars"
      ]
    },
    "ember-data": {
      "exports": "DS",
      "depends": [
        "ember:Ember"
      ]
    }
  },
  "hbsfy": {
    "extentions": ["hbs"],
    "precompiler": "ember-template-compiler",
    "compiler": "require('ember').Handlebars"
  },
  "private": true,
  "main": "server/index.js"
}
